<%- include('header') -%>



<form action="/pokemon/<%=index%>?_method=PUT" method="POST">

<input type="text" name="id" placeholder="id" value="<%=pokemon.id%>">
<br>
<input type="text" name="name" placeholder="name" value="<%=pokemon.name%>">
<br>
<input type="text" name="img" placeholder="img url" value="<%=pokemon.img%>">
